                    $('#createAccount').on('click', function() {
                        var _this = $(this);
                        $(this).attr('disabled', 'disabled');
                        var remoteParaminfo = {
                            "urlIdVal"      : $('#urlParamVal').val(),
                            "passwordVal"   : $('#password').val(),
                            "langCode"		: $('#hiddenLangCode').val()
                        };
                        //console.log('Remote Param Value 1- '+remoteParaminfo.urlIdVal);  
                        //console.log('Remote Param Value 2- '+remoteParaminfo.passwordVal);    
                        var password = $('#password').val();
                        var confirmPassword = $('#confirmPassword').val();
                        $('.processing').removeClass('ng-hide');
                        if(password === confirmPassword) {
                            Visualforce.remoting.Manager.invokeAction('{!$RemoteAction.Vitality_RegistrationRequestController.createAccount}', remoteParaminfo, function(result, event) {
                                console.log('Response is-'+JSON.stringify(result));
                                if(result.status === 'success') {
                                    window.location.href = '/VitalityRegistration/Vitality_Registration_Success?Email='+result.params;
                                }else if (result.status === 'error') {
                                    //console.log('@Result@'+result.status);
                                    //console.log(result.msg);
                                    $('.error-wrapper').show().removeClass('ng-hide').children('.alert-danger').children('p').html(result.msg);
                                    $('.processing').addClass('ng-hide');
                                    _this.removeAttr('disabled');
                                }
                            });
                        }
                    });
                 }); 
            	function checkFirst(event) { 
                    var node = getTarget(event); 
                    var result; 
                    
                    if( node.value == "" || $(node).hasClass('ng-invalid') ) {
                        var result =  "You must enter your first name"; 
                    } else {
                        result = ""; 
                    } // endif 
                    
                    showError(node, result); 
                    
                    return stopPropagation( event); 
                    
                }
                function getTarget( event ) { 
                    var e = event || window.event; 
                    
                    if( e.target ) 
                        return e.target; 
                    else 
                        return e.srcElement; 
                }
                function stopPropagation( event ) { 
                    if( event.stopPropagation ) 
                        event.stopPropagation(); 
                    
                    if( event.preventDefault ) 
                        event.preventDefault(); 
                    
                    if( event.cancelBubble) 
                        event.cancelBubble = true; 
                    
                    if(event.returnValue) 
                        event.returnValue = false; 
                    
                    return false; 
                } 
            	function showError(node, message) { 
                    var id = node.id + "-error"; 
                    
                    var node_error = document.getElementById( id ); 
                    
                    /* if( node_error ) 
                        node_error.innerHTML = "<span class='offscreen'>In valid: </span>" + message;  */
                    
                    if( message == "" ) { 
                        node.setAttribute('aria-invalid', 'false'); 
                        // node_error.style.display = "none"; 
                    } else { 
                        node.setAttribute('aria-invalid', 'true'); 
                        // node_error.style.display = "inline"; 
                    }
                }
            </script>
