                                        <label for="firstname" class="control-label">Prénom (facultatif)</label>
                                        <input type="text" data-ng-model="firstname" id="firstname" name="firstname" class="form-control neutral-error" data-ng-class="{'ng-invalid': errorServer.firstname}" data-ng-required="false" aria-required="false" aria-invalid="false" />
                                    </div>
                                    <div class="form-group col-sm-6">
                                        <label for="lastname" class="control-label">Nom de famille</label>
                                        <input aria-describedby="lastname-error" onblur="checkFirst(event)" type="text" manulife-lastname-validator="true" data-ng-model="lastname" id="lastname" name="lastname" class="form-control" data-ng-class="{'ng-invalid': errorServer.lastname}" data-ng-required="true" aria-required="true" aria-invalid="false" maxlength="80" size="80" />
                                        
