                                    <div class="col-sm-7 col-xs-12 ng-pristine ng-untouched ng-valid ng-binding ng-isolate-scope" data-mnlf-date-validator="true" data-empty="registerForm.isDateEmpty" data-form="registerForm" data-prefix="birthdate" data-ng-model="registerForm.isDateValid" aria-invalid="false">
