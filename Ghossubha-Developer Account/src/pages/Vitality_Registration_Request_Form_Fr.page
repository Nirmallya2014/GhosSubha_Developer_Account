            <script>
                $(document).ready(function() {
                    $('#btnSubmit').on('click', function() {
                        var _this = $(this);
                        $(this).attr('disabled', 'disabled');
                        var dateOfBirth = $('#birthdate-year').val() + '-' + $('#birthdate-month').val() + '-' + $('#birthdate-day').val();
                        var userDetails = {
                            "firstName"   : $('#firstname').val(),
                            "lastName"    : $('#lastname').val(),
                            "dateOfBirth" : dateOfBirth,
                            "policyNumber": $('#policyNumber').val(),
                            "email"       : $('#email').val()
                        };
                        
                        $('.processing').removeClass('ng-hide');
                        Visualforce.remoting.Manager.invokeAction('{!$RemoteAction.Vitality_RegistrationRequestController.checkContactExist}', userDetails, function(result, event) {
                            if (result.status === 'error') {
                                if(result.policyRelated === 'true') {
                                    $('.error-wrapper .alert-danger h3').hide();
                                    $('.error-wrapper').removeClass('ng-hide').children('.alert-danger').children('p').html("{!$Label.lbl_PolicyTerminated}");
                                }else {
                                    $('.error-wrapper .alert-danger h3').show();
                                    $('.error-wrapper').removeClass('ng-hide').children('.alert-danger').children('p').html("{!$Label.lbl_MatchRecord}");
                                }
                                $('.processing').addClass('ng-hide');
                                _this.removeAttr('disabled');
                            }
                            else if(result.status === 'success') {
                                window.location.href = '/VitalityRegistration/Vitality_Registration_PWD_Form_Fr?id='+result.id+'&email='+result.email;
                            }
                        });
                    });
                }); 
            	function checkFirst(event) { 
                    var node = getTarget(event); 
                    var result; 
                    
                    if( node.value == "" || $(node).hasClass('ng-invalid') ) {
                        var result =  "You must enter your first name"; 
                    } else {
                        result = ""; 
                    } // endif 
                    
                    showError(node, result); 
                    
                    return stopPropagation( event); 
                    
                }
                function getTarget( event ) { 
                    var e = event || window.event; 
                    
                    if( e.target ) 
                        return e.target; 
                    else 
                        return e.srcElement; 
                }
                function stopPropagation( event ) { 
                    if( event.stopPropagation ) 
                        event.stopPropagation(); 
                    
                    if( event.preventDefault ) 
                        event.preventDefault(); 
                    
                    if( event.cancelBubble) 
                        event.cancelBubble = true; 
                    
                    if(event.returnValue) 
                        event.returnValue = false; 
                    
                    return false; 
                } 
            	function showError(node, message) { 
                    var id = node.id + "-error"; 
                    
                    var node_error = document.getElementById( id ); 
                    
                    /* if( node_error ) 
                        node_error.innerHTML = "<span class='offscreen'>In valid: </span>" + message;  */
                    
                    if( message == "" ) { 
                        node.setAttribute('aria-invalid', 'false'); 
                        // node_error.style.display = "none"; 
                    } else { 
                        node.setAttribute('aria-invalid', 'true'); 
                        // node_error.style.display = "inline"; 
                    }
                }
            </script>
